{
   "name": "api-analise-satisfacao",
   "version": "1.0.0",
   "description": "API do sistema de análise da satifação desenvolvido por um projeto do IFMT campus Rondonópolis",
   "main": "dist/app.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "ts-node src/app.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:generate": "npm run typeorm -- migration:generate -d src/database/data-source.ts",
      "migration:run": "npm run typeorm -- migration:run -d src/database/data-source.ts",
      "migration:revert": "npm run typeorm -- migration:revert -d src/database/data-source.ts",
      "migration:show": "npm run typeorm -- migration:show -d src/database/data-source.ts"
   },
   "keywords": [
      "ifmt"
   ],
   "author": "",
   "license": "MIT",
   "dependencies": {
      "dotenv": "^16.4.5",
      "express": "^4.21.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   },
   "devDependencies": {
      "@types/express": "^5.0.0",
      "@types/node": "^16.11.10",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   }
}